{
  "$schema": "https://schema.tauri.app/config/2",
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "dragDropEnabled": false,
        "height": 600,
        "title": "AdbAutoPlayer",
        "visible": false,
        "width": 800
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "pnpm build",
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "features": [
      "pytauri/standalone"
    ],
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": false,
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "identifier": "com.AdbAutoPlayer.AdbAutoPlayer",
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/AdbAutoPlayer/AdbAutoPlayer/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDYxMjkwODlDNkRGQ0YwQzMKUldURDhQeHRuQWdwWVRkRUgzWnlDRk5qNm0zYkVYQ3RYZjV1UHpSMmpHTlNRVDRCbDdyR2JnaU8K"
    }
  },
  "productName": "AdbAutoPlayer",
  "version": "0.1.0"
}
