{
  "$schema": "https://schema.tauri.app/config/2",
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "dragDropEnabled": false,
        "height": 600,
        "title": "AdbAutoPlayer",
        "visible": false,
        "width": 800
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "pnpm build",
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "features": [
      "pytauri/standalone"
    ],
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": false,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "identifier": "com.AdbAutoPlayer.AdbAutoPlayer",
  "productName": "AdbAutoPlayer",
  "version": "0.1.0"
}
